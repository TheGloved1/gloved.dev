{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": false,
  "singleQuote": true,
  "bracketSpacing": true,
  "jsxBracketSameLine": false,
  "arrowParens": "always",
  "printWidth": 125,
  "proseWrap": "preserve",
  "endOfLine": "crlf",
  "overrides": [],
  "experimentalTernaries": true,
  "plugins": ["prettier-plugin-tailwindcss"],
  "insertPragma": false,
  "singleAttributePerLine": false,
  "jsxSingleQuote": true
}
